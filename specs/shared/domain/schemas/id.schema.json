{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "id.schema.json",
  "title": "Identifier",
  "type": "string",
  "minLength": 3,
  "maxLength": 64,
  "pattern": "^[a-z0-9][a-z0-9\\-]{2,63}$",
  "examples": [
    "proc-metrics-hourly",
    "dynatrace-prod",
    "connector-1"
  ]
}